    "@typescript-eslint/parser": "^5.59.11",
    "aws-cdk": "2.84.0",
    "aws-cdk-lib": "^2.84.0",
    "aws-sdk": "^2.1399.0",
    "aws-sdk-mock": "^5.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
